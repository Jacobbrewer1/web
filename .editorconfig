[*]
charset = utf-8
end_of_line = lf
indent_size = 4
indent_style = space
insert_final_newline = false
max_line_length = 120
tab_width = 4

[{*.bash,*.sh,*.zsh,*.proto,*.prisma}]
indent_size = 2
tab_width = 2

[{*.go,*.go2}]
ij_go_import_sorting = gofmt
ij_go_local_group_mode = prefix
ij_go_local_package_prefixes = github.com/jacobbrewer1

[{*.yaml,*.yml}]
indent_size = 2
